#include <iostream>
using namespace std;
int main()
{
	int Numofgames,count=0,counter=0;
	char Word;
	cin >> Numofgames;
	for (int i = 0; i < Numofgames; i++)
	{
		cin >> Word;
		if (Word== 'A')
			count++;
		else if (Word == 'D')
		{
			counter++;
		}
		else
		{
			count++;
			counter++;

		}
	}
	if (count > counter)
	{
		cout << "Anton";
	}
	else if (count == counter)
	{
		cout << "Friendship";
	}
	else
	{
		cout << "Danik";
	}


}
