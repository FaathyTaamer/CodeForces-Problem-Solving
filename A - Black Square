#include <bits/stdc++.h>
#define FIO	 ios_base::sync_with_stdio(false);cin.tie(NULL);
#define ll long long
#define v233Team
using namespace std;
int counter = 0;
int main()
{
    v233Team
        FIO
        int a1, a2, a3, a4; cin >> a1 >> a2 >> a3 >> a4;
    string numbers; cin >> numbers;
    for (int i = 0; i < numbers.size(); i++)
    {
        if (numbers[i] == '1')
            counter += a1;
        else if (numbers[i] == '2')
            counter += a2;
        else if (numbers[i] == '3')
            counter += a3;
        else if (numbers[i] == '4')
            counter += a4;
    }
    cout << counter;
}

