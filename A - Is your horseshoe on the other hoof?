#include <bits/stdc++.h>
#define FIO	 ios_base::sync_with_stdio(false);cin.tie(NULL);
#define ll long long
#define v233Team
using namespace std;
int counter = 0;
int main()
{
	v233Team
		FIO
		int n = 4;
	vector<int>vec(n);
	for (int i = 0; i < n; i++)
	{
		cin >> vec[i];
	}
	sort(vec.begin(), vec.end());
	for (int i = 0; i < n-1; i++)
	{
		if (vec[i] ==vec[i + 1])
			counter++;
	}
	cout << counter;
}
