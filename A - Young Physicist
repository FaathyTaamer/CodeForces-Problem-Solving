#include <bits/stdc++.h>
#define FIO	 ios_base::sync_with_stdio(false);cin.tie(NULL);
#define ll long long
#define v233Team
using namespace std;
int counter = 0;
int main()
{
    v233Team
        FIO
        int n; cin >> n;
    vector<int>v1(n);
    vector<int>v2(n);
    vector<int>v3(n);
    int c1=0, c2=0, c3=0;
    for (int i = 0; i < n; i++)
    {
        cin >> v1[i] >> v2[i] >> v3[i];
        c1 += v1[i];
        c2 += v2[i];
        c3 += v3[i];
    }
    if (c1==0&&c2==0&&c3==0)
        cout << "YES";
    else
        cout << "NO";
   
}

