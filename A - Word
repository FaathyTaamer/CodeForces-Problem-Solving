#include <iostream>
using namespace std;
int main()
{
	int counter = 0, number = 0;
	string x{"Fk"};
	cin >> x;
	for (int i = 0; i < x.length(); i++)
	{
		if (x[i] <= 90)
			counter++;
		else if (x[i] >= 97 )
			number++;
		else continue;
	}
	if (counter <= number || counter == number)
	{
		for (int i = 0; i < x.length(); i++)
		{
			x[i] = tolower(x[i]);
		}
		cout << x;
	
	}
	else
	{
		for (int i = 0; i < x.length(); i++)
		{
			x[i] = toupper(x[i]);
		}
		cout << x;
	}
}
