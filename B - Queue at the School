#include <bits/stdc++.h>
#define FIO	 ios_base::sync_with_stdio(false);cin.tie(NULL);
#define ll long long
#define v233Team
#define long long long
using namespace std;
int main()
{
    v233Team
        FIO
        int ns; cin >> ns;
    int t; cin >> t;
        string s; cin >> s;
    while (t--)
    {
        for (int i =0; i <s.size();i++)
        {
            if (s[i] == 'B' && s[i + 1] == 'G')
            {
                s[i] = 'G';
                s[i + 1] = 'B';
                i++;
            }
        }
    }
    cout << s;


}
