#include <bits/stdc++.h>
#define FIO	 ios_base::sync_with_stdio(false);cin.tie(NULL);
#define ll long long
#define v233Team
using namespace std;

int main()
{
    v233Team
        FIO
    int n;cin>>n;
    int arr[n],counter=1;
    for(int i =0; i <n;i++)
    {
        cin>>arr[i];
    }
    sort(arr , arr+n);
    for(int i =0 ; i <n;i++)
    {

        if(arr[i]==counter)
            counter++;
    }
    cout<<counter<<'\n';


}

