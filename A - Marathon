#include <bits/stdc++.h>
#define FIO	 ios_base::sync_with_stdio(false);cin.tie(NULL);
#define ll long long
#define v233Team
#define long long long
int counter = 0;
using namespace std;
int main()
{
    v233Team
        FIO
        int n; cin >> n;
    while (n--)
    {
        vector<int>vec(4);
        int c = 0;
        for (int i = 0; i < vec.size(); i++)cin >> vec[i];
        for (int i = 0; i < vec.size(); i++) { if (vec[i] > vec[0])c++; }
        cout << c<<'\n';
    }
}
